<?xml version="1.0" encoding="UTF-8"?>
<kcfg xmlns="http://www.kde.org/standards/kcfg/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.kde.org/standards/kcfg/1.0 http://www.kde.org/standards/kcfg/1.0/kcfg.xsd">
    <kcfgfile name=""/>

    <group name="General">
        <entry name="rowHeight" type="Int">
            <default>400</default>
        </entry>
        <entry name="rightColumnWidth" type="Int">
            <default>400</default>
        </entry>
        <entry name="leftColumnWidth" type="Int">
            <default>400</default>
        </entry>
    </group>
    <group name="Calendar">
        <entry name="main_calendar" type="String">
            <default>JA</default>
        </entry>
        <entry name="second_calendar" type="String">
            <default>GE</default>
        </entry>
        <entry name="show_events" type="Bool">
            <default>false</default>
        </entry>
        <entry name="events_json_files" type="String">
            <default></default> <!-- Store serialized JSON as a string -->
        </entry>
        <entry name="holiday_json_files" type="String">
            <default></default> <!-- Store serialized JSON as a string -->
        </entry>
    </group>
</kcfg>